<div class="container">
  <%= render 'public/shared/error_messages', resource: @tag %>
  <div class="row mt-5">
    <div class="col-md-8 mx-auto">
      <h4 class="text-center">タグ編集</h4>
      <%= form_with model: @tag, url: admin_tag_path(@tag), method: :patch do |f| %>
        <%= render "admin/sports/form", f: f %>
      <% end %>
    </div>
  </div>
</div>